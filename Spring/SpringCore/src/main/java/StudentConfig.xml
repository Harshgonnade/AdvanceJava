<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Create Student Objects - IOC -->
	<bean id="stud1"
		class="com.cdac.training.SpringCore.model.Student">

		<!-- Invoke Setter Methods - Setter DI -->
		
		<property name="rollNo" value="10001"></property>
		<property name="name" value="James Gosling"></property>
		<property name="college" value="VTU"></property>
		<property name="marks" value="74"></property>
	</bean>
	
	<bean id="stud2"
		class="com.cdac.training.SpringCore.model.Student">

		<!-- Invoke Setter Methods - Setter DI -->
		<property name="rollNo" value="10002"></property>
		<property name="name" value="Rod Johnson"></property>
		<property name="college" value="NITK"></property>
		<property name="marks" value="95"></property>
	</bean>


</beans>
